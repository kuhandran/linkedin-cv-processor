{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "api/index.py"
      },
      {
        "src": "/ui",
        "dest": "api/index.py"
      },
      {
        "src": "/health",
        "dest": "api/index.py"
      },
      {
        "handle": "filesystem"
      }
    ]
  }
  
  